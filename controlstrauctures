i = 1
while i < 6:
    print("I'm still going")
    i += 1

for i in range(50):
    if i > 24:
        break
    else:
        print(i)
    
car_color = "Blue"
car_make = "Tesla"

if car_color == "Blue":
    if car_make == "Tesla":
        print("My car is a Blue Tesla")
    else:
        print("My car is blue")
elif car_color == "Red":
    print("My car is a red ferrari")
else:
    print("I just have a bike")